<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="3000" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="dialog" alignment="CENTER_RIGHT" contentDisplay="TOP" styleClass="bubble" stylesheets="@../css/styles.css" text="Label" textAlignment="RIGHT" wrapText="true" HBox.hgrow="ALWAYS">
         <font>
            <Font name="Baloo Da 2 Regular" size="13.0" />
         </font></Label>
   </children>
</HBox>
